<div class="form-group">
  <%= f.label :ニックネーム %>
  <%= f.text_field :name, autofocus: true %>
</div>
<div class="form-group">
  <%= f.label :コメント %>
  <%= f.text_field :comment %>
</div>
<div class="form-group">
  <%= f.label :"E-mail" %>
  <%= f.text_field :email %>
</div>
<div class="form-group">
  <%= f.label :プロフィールイメージ %>
  <%= f.file_field :profile_image, accept: "image/*" %>
</div>